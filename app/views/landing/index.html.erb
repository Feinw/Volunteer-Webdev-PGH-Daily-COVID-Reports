<h1>data</h1>

<%# use this for notes https://chartkick.com/%>
<%= line_chart [{name: "with", data: @w_covid},{name: "pui", data: @pui},{name: "discharged", data: @discharged},{name: "expired", data: @expired}], curve: false, xtitle: "date", ytitle: "cases" %>


<div class="container">
    <table class="table">
    <tbody>
        <% @rows.each do |row| %>
            <tr>
                <% row.each do |col| %>
                    <td><%= col %></td>
                <% end %>
            </tr>
        <% end %>
    </tbody>
    </table>
</div>